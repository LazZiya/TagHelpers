<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <ApplicationIcon>Ziya-128.ico</ApplicationIcon>
    <Authors>Ziya Mollamahmut</Authors>
    <Company>Ziyad.info</Company>
    <Title>LazZiya.TagHelpers</Title>
    <Summary>Simple to use fully customizable pagination tag helper for ASP.NET Core 2.1 Razor projects.</Summary>
    <Description>Simple to use fully customizable pagination tag helper for ASP.NET Core 2.1 Razor projects.</Description>
    <Copyright>http://Ziya.info</Copyright>
    <PackageProjectUrl>https://github.com/LazZiya/TagHelpers</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/LazZiya/TagHelpers/blob/master/LICENSE</PackageLicenseUrl>
    <PackageIconUrl>https://github.com/LazZiya/TagHelpers/blob/master/LazZiya.TagHelpers/Ziya-128.ico</PackageIconUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageTags>asp.net-core, razor, taghelpers, pagination, email, phonenumber</PackageTags>
    <PackageReleaseNotes>Pagination TagHelper :
- minimum setup requirements
- fully customizable
- bootstrap 4.1 support
- support for large chuncks of pages and records</PackageReleaseNotes>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.TagHelpers" Version="2.1.2" />
  </ItemGroup>

</Project>
